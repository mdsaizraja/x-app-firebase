{
  "expo": {
    "name": "x-app",
    "slug": "x-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/logo.jpeg",
    "userInterfaceStyle": "light",
    "jsEngine": "jsc",
    "splash": {
      "image": "./src/assets/images/logo.jpeg",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.xapp",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/logo.jpeg",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.xapp"
    },
    "web": {
      "favicon": "./src/assets/images/logo.jpeg"
    },
    "plugins": [
      "react-native-edge-to-edge"
    ],
    "extra": {
      "eas": {
        "projectId": "912ddba4-4752-4fd9-8a7c-e5dcd3d51a96"
      }
    }
  }
}
